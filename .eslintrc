{
  "extends": ["@carlsberg/eslint-config-carlsberg"],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "@typescript-eslint/no-unused-vars-experimental": "off",
    "react/jsx-props-no-spreading": "off"
  },
  "overrides": [
    {
      "files": ["*.stories.tsx"],
      "rules": {
        "no-alert": "off"
      }
    },
    {
      "files": ["./scripts/**/*.mjs"],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
