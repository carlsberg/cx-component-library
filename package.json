{
  "name": "carlsberg-malty",
  "homepage": "https://malty.carlsberggroup.com/",
  "url": "https://malty.carlsberggroup.com",
  "version": "1.0.21",
  "private": true,
  "keywords": [
    "carlsberg",
    "malty",
    "design system",
    "component library"
  ],
  "description": "Malty, probably the best Design System in the world",
  "engines": {
    "node": ">=12 <=16",
    "npm": ">=6 <=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CarlsbergGBS/cx-component-library.git"
  },
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./build && rm -rf ./node_modules && rm -rf .git/bit && rm -f yarn.lock",
    "wipe": "yarn cache clean --force && bit clear-cache",
    "reset": "yarn clean && yarn wipe && yarn serve",
    "watch": "tsc -b -w --preserveWatchOutput",
    "compile": "tsc -b",
    "start": "bit compile && bit start",
    "serve": "bit install && bit compile && bit start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --docs -o docs",
    "build": "bit build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"malty/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-datepicker": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@carlsberg/eslint-config-carlsberg": "^0.0.1",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-docs": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/addons": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.0",
    "@storybook/manager-webpack5": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@storybook/theming": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/babel__core": "^7.1.14",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-hoist-jsx-style": "^1.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-functional": "*",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsdoc": "*",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.5.1",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "styled-components": "^5.2.3",
    "styled-normalize": "^8.0.7",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "workspaces": [
    "malty/*"
  ],
  "resolutions": {
    "styled-components": "^5",
    "node-forge": "^1.3.1",
    "glob-parent": "^5.1.2",
    "trim": "^0.0.3",
    "braces": "^2.3.2"
  },
  "bit": {
    "resolveModules": {
      "modulesDirectories": [
        "malty"
      ],
      "aliases": {
        "@": "malty"
      }
    }
  }
}
