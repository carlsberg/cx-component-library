{
  "name": "carlsberg-malty",
  "homepage": "https://malty.carlsberggroup.com/",
  "url": "https://malty.carlsberggroup.com",
  "version": "1.0.33",
  "private": true,
  "keywords": [
    "carlsberg",
    "malty",
    "design system",
    "component library"
  ],
  "description": "Malty, probably the best Design System in the world",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CarlsbergGBS/cx-component-library.git"
  },
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./build && rm -rf ./node_modules && rm -rf .git/bit",
    "wipe": "yarn cache clean --force && bit clear-cache",
    "reset": "yarn clean && yarn wipe && yarn install && bit install && yarn storybook",
    "watch": "tsc -b -w --preserveWatchOutput",
    "compile": "tsc -b",
    "start": "bit compile && yarn storybook",
    "serve": "bit install && bit compile && bit start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs",
    "build": "bit build",
    "test": "jest",
    "test:ci": "jest --silent --ci --coverage",
    "test:watch": "jest --watch",
    "format": "prettier --write . --loglevel error",
    "check-lint": "eslint \"malty/**/*.{js,ts,tsx}\" --max-warnings=9",
    "lint": "yarn check-lint --fix",
    "check-types": "tsc --noemit",
    "check-format": "prettier --check .",
    "set-peer-dep": "node ./scripts/set-peer-dependency.mjs"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@tanstack/react-table": "^8.7.6",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "^17.0.2",
    "react-popper": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@carlsberg/eslint-config-carlsberg": "^0.3.0",
    "@carlsberg/prettier-config": "^0.0.1",
    "@storybook/addon-actions": "^7.4.1",
    "@storybook/addon-docs": "^7.4.1",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/addons": "^7.4.1",
    "@storybook/react": "^7.4.1",
    "@storybook/react-webpack5": "^7.4.1",
    "@storybook/theming": "^7.4.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__core": "^7.1.20",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^17.0.18",
    "@types/react-table": "^7.7.14",
    "@types/react-test-renderer": "^17.0.2",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.53.0",
    "babel-jest": "^29.4.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-hoist-jsx-style": "^1.0.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-functional": "*",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "storybook": "^8.0.1",
    "styled-components": "^5.3.6",
    "styled-normalize": "^8.0.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "url-loader": "^4.1.1",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "workspaces": [
    "malty/*"
  ],
  "resolutions": {
    "styled-components": "^5",
    "node-forge": "^1.3.1",
    "glob-parent": "^5.1.2",
    "trim": "^0.0.3",
    "trim-newlines": "^3.0.1",
    "braces": "^2.3.2",
    "json5": "^2.2.2",
    "react": "<18.0.0",
    "react-dom": "<18.0.0",
    "@types/react": "<18.0.0",
    "@types/react-dom": "<18.0.0",
    "semver": "^7.5.2",
    "@adobe/css-tools": "4.3.2",
    "postcss": "8.4.31"
  },
  "bit": {
    "resolveModules": {
      "modulesDirectories": [
        "malty"
      ],
      "aliases": {
        "@": "malty"
      }
    }
  },
  "prettier": "@carlsberg/prettier-config"
}
