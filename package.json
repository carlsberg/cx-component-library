{
  "name": "carlsberg-malty",
  "homepage": "https://malty.carlsberggroup.com/",
  "url": "https://malty.carlsberggroup.com",
  "version": "1.0.33",
  "private": true,
  "keywords": [
    "carlsberg",
    "malty",
    "design system",
    "component library"
  ],
  "description": "Malty, probably the best Design System in the world",
  "engines": {
    "node": ">=12 <=16",
    "npm": ">=6 <=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CarlsbergGBS/cx-component-library.git"
  },
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./build && rm -rf ./node_modules && rm -rf .git/bit",
    "wipe": "yarn cache clean --force && bit clear-cache",
    "reset": "yarn clean && yarn wipe && yarn serve",
    "watch": "tsc -b -w --preserveWatchOutput",
    "compile": "tsc -b",
    "start": "bit compile && yarn storybook",
    "serve": "bit install && bit compile && bit start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --docs -o docs",
    "build": "bit build",
    "test": "jest",
    "test:ci": "jest --silent --ci",
    "test:watch": "jest --watch",
    "format": "prettier --write . --loglevel error",
    "check-lint": "eslint \"malty/**/*.{js,ts,tsx}\" --max-warnings=14",
    "lint": "yarn check-lint --fix",
    "check-types": "tsc --noemit",
    "check-format": "prettier --check ."
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tanstack/react-table": "^8.9.3",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.16.0",
    "react-dom": "^17.0.2",
    "react-popper": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@carlsberg/eslint-config-carlsberg": "^0.4.1",
    "@carlsberg/prettier-config": "^0.0.1",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-docs": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addons": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/react-webpack5": "^7.3.2",
    "@storybook/theming": "^7.3.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__core": "^7.20.1",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.21",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-datepicker": "^4.15.0",
    "@types/react-dom": "^17.0.20",
    "@types/react-table": "^7.7.15",
    "@types/react-test-renderer": "^17.0.2",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-jest": "^29.6.3",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-hoist-jsx-style": "^1.0.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-functional": "5.0.8",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsdoc": "*",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.1",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "prettier": "^3.0.2",
    "prettier-plugin-organize-imports": "^3.2.3",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.3.2",
    "styled-components": "^6.0.7",
    "styled-normalize": "^8.0.7",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6",
    "url-loader": "^4.1.1",
    "uuid": "^9.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "workspaces": [
    "malty/*"
  ],
  "resolutions": {
    "styled-components": "^5",
    "node-forge": "^1.3.1",
    "glob-parent": "^5.1.2",
    "trim": "^0.0.3",
    "trim-newlines": "^3.0.1",
    "braces": "^2.3.2",
    "json5": "^2.2.2",
    "react": "<18.0.0",
    "react-dom": "<18.0.0",
    "@types/react": "<18.0.0",
    "@types/react-dom": "<18.0.0",
    "semver": "^7.5.2",
    "@adobe/css-tools": "4.3.1"
  },
  "bit": {
    "resolveModules": {
      "modulesDirectories": [
        "malty"
      ],
      "aliases": {
        "@": "malty"
      }
    }
  },
  "prettier": "@carlsberg/prettier-config"
}
