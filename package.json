{
  "name": "carlsberg-malty",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./build && rm tsconfig.tsbuildinfo",
    "watch": "tsc -b -w --preserveWatchOutput",
    "compile": "tsc -b",
    "start": "bit compile && bit start",
    "build": "bit install && bit compile && bit start",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook --docs -o docs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"malty/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@storybook/react": "^6.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@carlsberg/eslint-config-carlsberg": "^0.0.1",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-storysource": "^6.3.6",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/babel__core": "^7.1.14",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-functional": "*",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "*",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "uuid": "^8.3.2"
  },
  "workspaces": [
    "malty/*"
  ],
  "resolutions": {
    "styled-components": "^5"
  },
  "bit": {
    "resolveModules": {
      "modulesDirectories": [
        "malty"
      ],
      "aliases": {
        "@": "malty"
      }
    }
  }
}
